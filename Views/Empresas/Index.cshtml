@model IEnumerable<ProyectoLlaves.Models.Empresas>

@{
    ViewBag.Title = "Index";
}

<h2>Registro de Empresas</h2>

<p>
    @Html.ActionLink("Create New", "Create",new { }, new { @class="btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmpresasID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmpresasID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmpresasID })
        </td>
    </tr>
}

</table>
